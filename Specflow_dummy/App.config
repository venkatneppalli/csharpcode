<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <specFlow>
    
    
    
    <!-- use unit test provider SpecRun+NUnit or SpecRun+MsTest for being able to execute the tests with SpecRun and another provider -->
    <!-- <unitTestProvider name="NUnit" />-->
    <unitTestProvider name="SpecRun" />
    <plugins>
      <add name="SpecRun" />
    </plugins>
  </specFlow>
  <appSettings>

    <add key="url" value="http://adactin.com/HotelApp/" />
    <add key="loginusername" value="//input[@id='username']" />

    <add key="loginpassword" value="//input[@id='password']" />

    <add key="loginbutton" value="//*[@id='login']" />



  </appSettings>

</configuration>